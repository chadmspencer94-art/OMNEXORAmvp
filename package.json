{
  "name": "omnexora-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "prisma generate && next build --webpack",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "export:users": "npx ts-node --project scripts/tsconfig.json scripts/exportUsersFromPostgres.ts",
    "import:users": "npx ts-node --project scripts/tsconfig.json scripts/importUsersToSqlite.ts",
    "db:sync": "prisma db push --schema ./prisma/schema.prisma --force-reset",
    "db:gen": "prisma generate --schema ./prisma/schema.prisma",
    "demo:seed": "node ./scripts/demo-seed.mjs"
  },
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "@vercel/kv": "^3.0.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.4.7",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.555.0",
    "next": "^16.1.0-canary.16",
    "openai": "^6.9.1",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "resend": "^6.5.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "prisma": "^6.19.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
